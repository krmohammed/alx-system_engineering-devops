global
        log 127.0.0.1 local0 notice
        maxconn 1024
        user haproxy
        group haproxy
defaults
        log     global
        mode    http
        option  httplog
        option  dontlognull
	retries 3
	option http-server-close
	option http-pretend-keepalive
	option redispatch
        timeout connect 5000
        timeout client  50000
        timeout server  50000
frontend web_frontend
	bind *:443 ssl crt /etc/ssl/certs/www.thekrmohammed.tech
        default_backend my_servers
backend my_servers
        balance roundrobin
        server 310495-web-01 35.153.57.28 check port 80
        server 310495-web-02 34.232.71.62 check port 80
